{
  "name": "baste",
  "version": "0.0.17",
  "license": "MIT",
  "author": "Isaac Graves",
  "description": "asynchronous jsx templating",
  "repository": {
    "url": "https://github.com/ibgrav/baste"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist",
    "jsx-runtime"
  ],
  "types": "./dist/index.d.ts",
  "main": "./dist/index.umd.js",
  "module": "./dist/index.mjs",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "default": "./dist/index.mjs",
      "require": "./dist/index.umd.js"
    },
    "./jsx-runtime": {
      "types": "./dist/jsx-runtime.d.ts",
      "import": "./dist/jsx-runtime.mjs",
      "default": "./dist/jsx-runtime.mjs",
      "require": "./dist/jsx-runtime.umd.js"
    }
  },
  "scripts": {
    "dev": "concurrently \"yarn build:rollup --watch\" \"yarn build:tsc --watch\"",
    "build": "yarn build:rollup && yarn build:tsc",
    "build:tsc": "tsc --preserveWatchOutput -p tsconfig.build.json",
    "build:rollup": "rollup -c rollup.config.js",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "pub": "yarn build && np"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^8.3.3",
    "c8": "^7.11.3",
    "concurrently": "^7.2.2",
    "prettier": "^2.7.1",
    "rollup": "^2.75.7",
    "rollup-plugin-terser": "^7.0.2",
    "tslib": "^2.4.0",
    "typescript": "^4.7.4",
    "vitest": "^0.17.0"
  }
}
